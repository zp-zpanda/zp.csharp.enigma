{
  "metadata" : [
    {
      "src" : [
        {
          "files" : [
            "ZP.CSharp.Enigma.dll",
            "ZP.CSharp.Enigma.Cli.dll",
            "ZP.CSharp.Enigma.Docs.dll",
            "ZP.CSharp.Enigma.Tests.dll"
          ],
          "src" : "./docs-bin"
        }
      ],
      "dest" : "api",
      "disableGitFeatures" : false,
      "disableDefaultFilter" : false
    }
  ],
  "build" : {
    "content" : [
      {
        "files" : [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files" : [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource" : [
      {
        "files" : [
          "images/**"
        ]
      }
    ],
    "overwrite" : [
      {
        "files" : [
          "apidoc/**.md"
        ],
        "exclude" : [
          "obj/**",
          "docs/**",
          "old-docs/**"
        ]
      }
    ],
    "dest" : "docs",
    "globalMetadata" : {
      "_appTitle" : "ZP.CSharp.Enigma",
      "_gitContribute" : {
        "repo" : "https://github.com/zp-zpanda/zp.csharp.enigma",
        "branch" : "main"
      }
    },
    "globalMetadataFiles" : [],
    "fileMetadataFiles" : [],
    "template" : [
      "statictoc",
      "memberpage"
    ],
    "postProcessors" : [],
    "markdownEngineName" : "markdig",
    "noLangKeyword" : false,
    "keepFileLink" : false,
    "cleanupCacheHistory" : false,
    "disableGitFeatures" : false,
    "force" : true
  }
}